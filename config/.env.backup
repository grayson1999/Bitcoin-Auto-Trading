# .env.backup
# 실제 값은 .env 파일에만 두고, 이 파일은 예시용으로 사용하세요.

# Upbit API 인증 정보
UPBIT_ACCESS=        # Upbit Access Key
UPBIT_SECRET=        # Upbit Secret Key

# OpenAI API 인증 정보
OPENAI_API_KEY=      # OpenAI API Key (예: sk-proj-...)

# MySQL 실시간 DB 접속 정보
REALTIME_DB_USER=    # 사용자 이름 (예: bitcoin)
REALTIME_DB_PASS=    # 비밀번호 
REALTIME_DB_HOST=    # 호스트 (기본: localhost)
REALTIME_DB_PORT=    # 포트 (기본: 3306)
REALTIME_DB_NAME=    # 데이터베이스 이름 (예: bitcoindb_realtime)

# MySQL 히스토리 DB 접속 정보
HISTORY_DB_USER=     # 사용자 이름 (예: history)
HISTORY_DB_PASS=     # 비밀번호 
HISTORY_DB_HOST=     # 호스트 (기본: localhost)
HISTORY_DB_PORT=     # 포트 (기본: 3306)
HISTORY_DB_NAME=     # 데이터베이스 이름 (예: historydb_history)
